addedParams) {\r\n      newParams.append(...addedParams);\r\n    }\r\n    if (newParams.toString()) {\r\n      if (newURL.indexOf('?') !== -1) {\r\n        newURL += '\u0026' + newParams.toString();\r\n      } else {\r\n        newURL += '?' + newParams.toString();\r\n      }\r\n    }\r\n\r\n    return newURL;\r\n  }\r\n\r\n  document.addEventListener('DOMContentLoaded', () =\u003e {\r\n    // PROMO BANNER\r\n    // Data\r\n    const promoBannerBtn = document.getElementById('promo-banner-btn');\r\n\r\n    promoBannerBtn.addEventListener('click', () =\u003e {\r\n      window.dataLayer.push({\r\n        event: 'ctaClick',\r\n        metadata: {\r\n          linkText: 'Get the Deal',\r\n          linkURL: promoBannerBtn.href,\r\n          moduleName: 'Footer',\r\n          bandName: 'Promo Banner',\r\n          eventType: 'CTA clicked',\r\n          eventCategory: 'Sign Up CTA'\r\n        }\r\n      });\r\n\r\n      if (window.location.search) {\r\n        promoBannerBtn.href = constructNewURL(promoBannerBtn.href);\r\n      }\r\n    });\r\n\r\n    // Countdown\r\n    if (document.querySelector('.promo-countdown')) {\r\n\r\n      // Be careful of daylights savings EST vs EDT\r\n      const countDownDate = new Date('December 3, 2024 03:00:00 GMT-05:00').getTime();\r\n\r\n      let countDays = document.getElementById('count-days');\r\n      let countHours = document.getElementById('count-hours');\r\n      let countMinutes = document.getElementById('count-mins');\r\n      let countSeconds = document.getElementById('count-secs');\r\n\r\n      // update count down every second\r\n      let x = setInterval(() =\u003e {\r\n        let now = new Date().getTime();\r\n        let timeLeft = countDownDate - now;\r\n\r\n        // if countdown finished\r\n        if (timeLeft \u003c 0) {\r\n          clearInterval(x);\r\n          let countdownDiv = document.querySelector('.promo-countdown');\r\n\r\n          countdownDiv.classList.add('d-none');\r\n          countdownDiv.nextElementSibling.classList.remove('d-none');\r\n        } else {\r\n          // time calculations\r\n          let days = Math.floor(timeLeft / (1000 * 60 * 60 * 24));\r\n          let hours = Math.floor((timeLeft % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));\r\n          let minutes = Math.floor((timeLeft % (1000 * 60 * 60)) / (1000 * 60));\r\n          let seconds = Math.floor((timeLeft % (1000 * 60)) / 1000);\r\n\r\n          // display results\r\n          countDays.innerHTML = days;\r\n          countHours.innerHTML = hours;\r\n          countMinutes.innerHTML = minutes;\r\n          countSeconds.innerHTML = seconds;\r\n        }\r\n      }, 1000);\r\n    }\r\n\r\n    // Animation\r\n    const promoBanner = document.getElementById('promo-banner');\r\n\r\n    const makeStickyTop = () =\u003e {\r\n      if (window.scrollY \u003e= 136) {\r\n        promoBanner.classList.add('sticky-top');\r\n      } else {\r\n        promoBanner.classList.remove('sticky-top');\r\n      }\r\n    }\r\n\r\n    const makeStickyBottom = (elTrigger) =\u003e {\r\n      if (elTrigger.getBoundingClientRect().bottom \u003c 0) {\r\n        promoBanner.classList.add('sticky-bottom');\r\n      } else {\r\n        promoBanner.classList.remove('sticky-bottom');\r\n      }\r\n    }\r\n\r\n    window.onscroll = () =\u003e {\r\n      makeStickyTop();\r\n    }\r\n\r\n    // Modal\r\n    const termsModal = document.getElementById('promo-terms-modal');\r\n    const termsLinks = document.querySelectorAll('a[href=\"/a/black-friday-2024-terms\"]');\r\n    const termsModalCloseBtn = termsModal.querySelector('.close');\r\n    let lastFocusedElement;\r\n\r\n    const handleEscKey = (e) =\u003e {\r\n      if (e.key === 'Escape' || e.key === 'Esc') {\r\n        e.preventDefault();\r\n        termsModalCloseBtn.click();\r\n      }\r\n    };\r\n\r\n    const handleModalOpen = (e) =\u003e {\r\n      e.preventDefault();\r\n      lastFocusedElement = e.target;\r\n      document.body.classList.add('modal-open');\r\n      termsModal.style.display = 'block';\r\n      termsModal.focus();\r\n      document.addEventListener('keydown', handleEscKey);\r\n    }\r\n\r\n    const handleModalClose = (e) =\u003e {\r\n      document.body.classList.remove('modal-open');\r\n      document.removeEventListener('keydown', handleEscKey);\r\n      termsModal.style.display = 'none';\r\n      if (lastFocusedElement) {\r\n        lastFocusedElement.focus();\r\n        lastFocusedElement = '';\r\n      }\r\n    }\r\n\r\n    termsLinks.forEach((el) =\u003e {\r\n      el.addEventListener('click', (e) =\u003e {\r\n        handleModalOpen(e);\r\n      });\r\n    });\r\n\r\n    termsModalCloseBtn.addEventListener('click', (e) =\u003e {\r\n      handleModalClose(e);\r\n    });\r\n\r\n    window.addEventListener('click', (e) =\u003e {\r\n      if (e.target === termsModal) {\r\n        handleModalClose(e);\r\n      }\r\n    });\r\n  });\r\n\u003c/script\u003e\r\n\u003cstyle\u003e\r\nbody {\r\n background-color: #060212; \r\n}\r\narticle.w-100 { \r\n  background-attachment: initial;\r\n  background: linear-gradient(48deg, #070739 0%, #060212 35%);\r\n}\r\n.max-section-collection-hero-parent {\r\n    position: relative;\r\n    background-position: center top;\r\n}\r\n.max-section-collection-hero-parent::before {\r\n    background: linear-gradient(180deg, rgba(0, 0, 0, 0.6) 0%, rgba(0, 0, 0, 0) 19.6%), radial-gradient(97.07% 97.07% at 60.4% -13.07%, rgba(0, 0, 0, 0) 56.64%, #060212 100%);\r\n    content: \"\";\r\n    height: 100%;\r\n    position: absolute;\r\n    width: 100%;\r\n    z-index: 0;\r\n}\r\n.max-section-collection-hero .layout {\r\n  max-height: 100vh;\r\n  height: 900px;\r\n  padding-bottom: 1rem;\r\n}\r\n.max-section-collection-hero .column.column-content {\r\n  padding-bottom: 2rem;\r\n  order: 3;\r\n}\r\n.max-section-collection-hero .column.column-callout {\r\n  order: 1;\r\n}\r\n.max-section-collection-hero .column.column-content p {\r\n  font-size: 1.2rem;\r\n  line-height: 1.5;\r\n}\r\n.max-secti